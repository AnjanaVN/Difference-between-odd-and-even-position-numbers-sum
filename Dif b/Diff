#include<stdio.h>
#include<stdlib.h>
void main()
{
char str[50];
int i,os=0,es=0,diff;
printf("enter number");
scanf("%s",str);
for(i=0;str[i];i++)
{
if(i%2==0)
{
es=es+str[i];
}
else
{
os=os+str[i];
}
}
diff=abs(os-es);
printf("difference in the sum of odd and even position numbers=%d",diff);
}
MISSING ELEMENT IN ARRAY
#include<stdio.h>
void main()
{
int n,i,arr[20],b,c;
printf("enter limit");
scanf("%d",&n);
printf("enter elements");
for(i=0;i<n;i++)
{
scanf("%d",&arr[i]);
}
c=arr[0];
for(i=0;i<n;i++)
{
 if(arr[i]!=c+i)
 {
 b=c+i;
 }
}
printf("%d",b);
}
